{
    "name": "uni-virtualizer-examples",
    "version": "0.1.0",
    "description": "Examples demonstrating the functionality of the uni-virtualizer libraries",
    "license": "BSD-3-Clause",
    "repository": "PolymerLabs/uni-virtualizer",
    "private": true,
    "scripts": {
        "build:virtualizer": "npm run build --prefix ../lit-virtualizer",
        "build": "npm run build:virtualizer && rollup -c",
        "build-and-watch": "npm run build:virtualizer && rollup -c -w",
        "clean": "rm -r node_modules/",
        "start": "npm run build && http-server"
    },
    "author": "The Polymer Authors",
    "devDependencies": {
        "http-server": "^0.11.1",
        "rollup": "^1.11.2",
        "rollup-plugin-node-resolve": "^4.2.3",
        "rollup-plugin-terser": "^5.0.0"
    },
    "dependencies": {
        "lit-element": "latest"
    }
}
